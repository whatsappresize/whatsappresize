{
  "name": "WhatsApp Web Messagebox Resizer",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [],
  "host_permissions": [],
  "action": {
    "default_icon": "resize.png"
  },
  "content_scripts": [
    {
      "js": ["script/resizer.js"],
      "css": ["css/resizer.css"],
      "matches": [
        "https://web.whatsapp.com/*"
      ]
    }
  ]
}
